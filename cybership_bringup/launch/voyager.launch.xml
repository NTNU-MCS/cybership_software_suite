<launch>
  <include file="$(find-pkg-share cybership_bringup)/launch/include/azimuth_controller.launch.xml">
    <arg name="vessel_model" value="voyager"/>
    <arg name="vessel_name" value="voyager"/>
    <arg name="param_file" value="$(find-pkg-share cybership_config)/config/voyager/azimuth_controller.yaml"/>
  </include>

  <include file="$(find-pkg-share cybership_bringup)/launch/include/motion_capture_system_connector.launch.py">
    <arg name="vessel_model" value="voyager"/>
    <arg name="vessel_name" value="voyager"/>
    <arg name="param_file" value="$(find-pkg-share cybership_config)/config/voyager/mocap_connector.yaml"/>
  </include>

  <include file="$(find-pkg-share cybership_bringup)/launch/include/motion_capture_system_transformer.launch.xml">
    <arg name="vessel_model" value="voyager"/>
    <arg name="vessel_name" value="voyager"/>
    <arg name="param_file" value="$(find-pkg-share cybership_config)/config/voyager/mocap_transformer.yaml"/>
  </include>

  <include file="$(find-pkg-share cybership_bringup)/launch/include/force_multiplexer.launch.xml">
    <arg name="vessel_model" value="voyager"/>
    <arg name="vessel_name" value="voyager"/>
    <arg name="param_file" value="$(find-pkg-share cybership_config)/config/voyager/force_multiplexer.yaml"/>
  </include>

  <include file="$(find-pkg-share cybership_bringup)/launch/include/servo_driver.launch.xml">
    <arg name="vessel_model" value="voyager"/>
    <arg name="vessel_name" value="voyager"/>
    <arg name="param_file" value="$(find-pkg-share cybership_config)/config/voyager/servo_driver.yaml"/>
  </include>

  <include file="$(find-pkg-share cybership_bringup)/launch/include/thruster_control.launch.xml">
    <arg name="vessel_model" value="voyager"/>
    <arg name="vessel_name" value="voyager"/>
    <arg name="param_file" value="$(find-pkg-share cybership_config)/config/voyager/thruster_control.yaml"/>
  </include>

  <include file="$(find-pkg-share cybership_bringup)/launch/include/imu_bno055.launch.xml">
    <arg name="vessel_model" value="voyager"/>
    <arg name="vessel_name" value="voyager"/>
    <arg name="param_file" value="$(find-pkg-share cybership_config)/config/voyager/imu_bno055.yaml"/>
  </include>
</launch>
